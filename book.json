{
    "title": "Documentation of the Poppy Platform",

    "plugins": [
        "-share",
        "versions", 
        "github", 
        "mathjax",
        "numbered-headings", 
        "printlinks", 
        "copy-code-button", 
        "youtubex"
    ],
    "pluginsConfig": {
        "versions": {
            "type": "languages"
        },
        "github": {
            "url": "https://github.com/poppy-project/poppy-docs"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper"
            ]
        }
    },
    "variables": {
        "poppy_doc_version": "1.0"
    }
    
}
